[
  {
    "cmd": [
      "python",
      "-u",
      "None/tools/perf/run_benchmark",
      "list",
      "--browser",
      "sample-browser",
      "--json-output",
      "/path/to/tmp/json",
      "--num-shards",
      "4",
      "--device",
      "device-id"
    ],
    "env": {
      "CHROME_HEADLESS": "1"
    },
    "name": "List Perf Tests",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@{@@@",
